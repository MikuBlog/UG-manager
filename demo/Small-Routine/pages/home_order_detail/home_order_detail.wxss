.order-detail {
  position: relative;
  background: #f4f4f4;
}

.order-detail-box {
  position: relative;
  text-align: center;
  overflow: hidden;
}

.order-box-form {
  position: relative;
  text-align: left;
}

.order-head {
  position: relative;
  color: #6d6d6d;
  font-size: 18px;
  margin: 25rpx 0 25rpx 25rpx;
}

.order-price-box {
  position: relative;
  background: #fff;
  overflow: hidden;
}

.order-price {
  margin: 35rpx 0 35rpx 25rpx;
  font-size: 35rpx;
}

.order-status-box {
  position: relative;
  background: #fff;
  overflow: hidden;
}

.order-status {
  margin: 35rpx 0 35rpx 25rpx;
  font-size: 35rpx;
}

.order-position-box {
  position: relative;
  background: #fff;
  overflow: hidden;
}

.order-position {
  margin: 14rpx 0 14rpx 25rpx;
  font-size: 35rpx;
}

.order-person {
  margin: 14rpx 0 10rpx 25rpx;
  font-size: 30rpx;
  color: #9e9e9e;
}

.order-person span {
  margin-right: 25rpx;
}

.order-time-box {
  position: relative;
  background: #fff;
  overflow: hidden;
}

.order-time {
  margin: 35rpx 0 35rpx 25rpx;
  font-size: 35rpx;
  color: #000;
}

.order-type-box {
  position: relative;
  background: #fff;
  overflow: hidden;
}

.order-type {
  margin: 35rpx 0 35rpx 25rpx;
  font-size: 35rpx;
  color: #000;
}

.order-description-box {
  position: relative;
  background: #fff;
  overflow: hidden;
}

.order-description {
  margin: 35rpx 0 35rpx 25rpx;
  font-size: 35rpx;
}

.order-remark-box {
  position: relative;
  background: #fff;
  overflow: hidden;
}

.order-remark {
  margin: 35rpx 0 35rpx 25rpx;
  font-size: 35rpx;
}

.order-photo-box {
  position: relative;
  overflow: hidden;
  background: #fff;
  height: 300rpx;
  line-height: 300rpx;
}

image {
  position: relative;
  vertical-align: middle;
  margin-left: 20rpx;
  width: 200rpx;
  height:200rpx
}

.order-button-box {
  position: relative;
  text-align: center;
}

.order-button {
  position: relative;
  margin: 30rpx auto;
  width: 90%;
  line-height: 90rpx;
  font-size: 30rpx;
  font-weight: bold;
  background-color: #33689b;
  border-radius: 10rpx;
  color: #fff;
}

.order-history-box {
  position: relative;
  overflow: hidden;
  color: #9e9e9e;
}

.order-history-head {
  margin: 35rpx 0 35rpx 25rpx;
  font-size: 35rpx;
}

.order-history-list {
  margin: 35rpx 0 35rpx 25rpx;
  font-size: 30rpx;
}

.order-box-warn {
  position: fixed;
  text-align: center;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  z-index: 99;
  background: rgba(0, 0, 0, 0.5);
}

.warn-box {
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  margin: auto;
  width: 580rpx;
  height: 300rpx;
  background: #fff;
  border-radius: 10rpx;
}

.warn-head {
  position: relative;
  font-size: 30rpx;
  font-weight: bold;
  margin-top: 60rpx;
}

.warn-content {
  position: relative;
  color: #989898;
  margin-top: 20rpx;
  font-size: 30rpx;
}

.warn-button {
  position: absolute;
  bottom: 0;
  width: 100%;
  display: flex;
  border-top: 1px solid #dfdfe1;
  font-size: 35rpx;
}

.warn-button   view {
  position: relative;
  flex: 1;
  line-height: 80rpx;
}

.warn-button      view:first-of-type {
  color: #a9a9a9;
}

.warn-button      view:last-of-type {
  color: #07c319;
}

.form-box {
  border-top: 1px solid #e6e6e6;
  border-bottom: 1px solid #e6e6e6;
}
